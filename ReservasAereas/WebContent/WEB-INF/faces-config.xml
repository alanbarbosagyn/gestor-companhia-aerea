<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<managed-bean>
		<managed-bean-name>reservaBusiness</managed-bean-name>
		<managed-bean-class>business.ReservaBusiness</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cliente</managed-bean-name>
		<managed-bean-class>cliente.Cliente</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>passageiro</managed-bean-name>
		<managed-bean-class>passageiro.Passageiro</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reserva</managed-bean-name>
		<managed-bean-class>reserva.Reserva</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>voo</managed-bean-name>
		<managed-bean-class>voo.Voo</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>escala</managed-bean-name>
		<managed-bean-class>escala.Escala</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>aeroporto</managed-bean-name>
		<managed-bean-class>aeroporto.Aeroporto</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>vooCompleto</managed-bean-name>
		<managed-bean-class>voo.VooCompleto</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/dashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
